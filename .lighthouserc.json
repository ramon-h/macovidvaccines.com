{
  "ci": {
    "collect": {
        "staticDistDir": "./build",
        "numberOfRuns": 1
    },
    "assert" :{
      "preset": "lighthouse:no-pwa",
      "assertions": {
        "uses-text-compression": "off",
        "unused-javascript": "off"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
